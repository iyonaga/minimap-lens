{
  "name": "minimap-lens",
  "main": "./lib/minimap-lens",
  "version": "0.7.0",
  "description": "Show code lens on hovering to Minimap.",
  "keywords": [
    "minimap",
    "lens",
    "lens-mode"
  ],
  "repository": "https://github.com/iyonaga/minimap-lens",
  "license": "MIT",
  "scripts": {
    "lint": "eslint '{lib,spec}/**/*.js' --format codeframe",
    "lint:fix": "prettier-eslint --write '{lib,spec}/**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "minimap": {
      "versions": {
        "1.0.0": "consumeMinimapServiceV1"
      }
    }
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "prettier-eslint-cli": "^4.7.1"
  }
}
